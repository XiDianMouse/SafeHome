<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/colorTheme">
            </stroke>
            <corners
                android:radius="3333dp">
            </corners>
            <padding
                android:bottom="12dp"
                android:left="12dp"
                android:right="12dp"
                android:top="12dp">
            </padding>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/colorTheme">
            </stroke>
            <corners
                android:radius="3333dp">
            </corners>
            <padding
                android:bottom="12dp"
                android:left="12dp"
                android:right="12dp"
                android:top="12dp">
            </padding>
            <solid
                android:color="@color/colorTheme">
            </solid>
        </shape>
    </item>
</selector>